{
  "name": "@account-kit/react-native",
  "version": "4.13.0",
  "description": "Components and hooks for using Account Kit in React Native",
  "author": "Alchemy",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "src/**/*.ts",
    "src/**/*.tsx",
    "!dist/**/*.tsbuildinfo",
    "!vitest.config.ts",
    "!.env",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "!src/__tests__/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "prebuild": "tsx ./inject-version.ts",
    "build": "yarn clean && yarn build:esm && yarn build:types",
    "build:esm": "tsc --project tsconfig.build.json --outDir ./dist/esm",
    "build:types": "tsc --project tsconfig.build.json --declarationDir ./dist/types --emitDeclarationOnly --declaration --declarationMap",
    "docs:gen": "node ../../doc-gen/dist/esm/cli.js generate --in ./src/index.ts --out ../../site/pages/reference/account-kit/react-native",
    "clean": "rm -rf ./dist",
    "test": "vitest --passWithNoTests",
    "test:run": "vitest run --passWithNoTests"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.28.9",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.45",
    "react": "^18.2.0",
    "typescript": "^5.0.4",
    "typescript-template": "*",
    "vitest": "^2.0.4"
  },
  "dependencies": {
    "@account-kit/infra": "^4.13.0",
    "@account-kit/logging": "^4.13.0",
    "@account-kit/signer": "^4.13.0",
    "@account-kit/core": "^4.13.0",
    "@account-kit/react-native-signer": "^4.13.0",
    "@account-kit/react": "^4.13.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.28.9",
    "react": "^18.2.0"
  },
  "resolutions": {
    "viem": "2.22.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alchemyplatform/aa-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/alchemyplatform/aa-sdk/issues"
  },
  "homepage": "https://github.com/alchemyplatform/aa-sdk#readme",
  "gitHead": "ee46e8bb857de3b631044fa70714ea706d9e317d",
  "optionalDependencies": {
    "alchemy-sdk": "^3.0.0"
  }
}
