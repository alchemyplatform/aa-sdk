{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test", "generate"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build", "generate"],
      "outputs": ["{projectRoot}/dist"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "generate": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/src/plugins"]
    }
  }
}
