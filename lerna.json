{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "5.0.0-beta.0",
  "npmClient": "yarn",
  "packages": [
    "packages/aa-infra",
    "packages/common",
    "packages/smart-accounts",
    "packages/wallet-apis"
  ],
  "command": {
    "version": {
      "message": "chore(v5): publish beta version %s [skip ci]",
      "forcePublish": true,
      "conventionalCommits": false,
      "preid": "beta"
    },
    "publish": {
      "distTag": "latest",
      "access": "public"
    }
  }
}
