{
  "name": "root",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.16"
  },
  "packageManager": "yarn@1.19.0",
  "workspaces": {
    "packages": [
      "aa-sdk/*",
      "account-kit/*",
      "libs/*",
      "examples/*",
      "site",
      "doc-gen",
      ".vitest"
    ]
  },
  "resolutions": {
    "wagmi": "2.12.7",
    "viem": "2.20.0"
  },
  "scripts": {
    "generate": "lerna run generate",
    "postgenerate": "yarn lint:write",
    "preinstall": "yarn config set ignore-engines true",
    "postinstall": "git submodule update --init --recursive && lerna run postinstall",
    "build": "is-ci && yarn build:ci || yarn build:dev",
    "build:base": "lerna run build --verbose --ignore=alchemy-daapp --ignore=embedded-accounts-quickstart --ignore=aa-simple-dapp --ignore=ui-demo",
    "build:dev": "yarn build:base --ignore=docs",
    "build:ci": "yarn build:base && yarn lint:write",
    "build:examples": "lerna run build",
    "clean": "lerna run clean",
    "docs:gen": "lerna run docs:gen",
    "test": "vitest dev",
    "test:ci": "vitest run",
    "test:e2e": "lerna run --verbose test:run-e2e --concurrency=1 --ignore=@alchemy/aa-signers",
    "lint:write": "eslint . --fix && yarn docs:gen && prettier --write --ignore-unknown .",
    "lint:check": "eslint . && prettier --check .",
    "lint:ci:eslint": "eslint --format github $(git diff --name-only origin/main HEAD | xargs)",
    "prepare": "husky install",
    "version": "yarn build:ci",
    "storybook": "nx storybook @account-kit/react",
    "build-storybook": "nx build-storybook project-name",
    "test-storybook": "nx test-storybook project-name"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@nx/js": "19.6.5",
    "@nx/react": "19.6.5",
    "@nx/storybook": "19.6.5",
    "@nx/vite": "19.6.5",
    "@nx/web": "19.6.5",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/core-server": "^8.2.8",
    "@storybook/jest": "^0.2.3",
    "@storybook/react-vite": "^8.2.8",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tanstack/react-query": "^5.54.1",
    "@types/prettier": "^2.7.3",
    "add": "^2.0.6",
    "ak-docgen": "*",
    "eslint": "^8.39.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-formatter-github": "^1.1.4",
    "eslint-formatter-rdjson": "^1.0.6",
    "eslint-plugin-eslint-rules": "*",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-mdx": "^3.1.5",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "lerna": "^8.1.8",
    "lint-staged": "^13.2.2",
    "node-fetch": "^3.3.1",
    "nx": "^19.6.5",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.8",
    "prool": "^0.0.15",
    "storybook": "^8.2.8",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "^5.0.4",
    "viem": "2.20.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.4",
    "wagmi": "2.12.7",
    "webauthn-p256": "^0.0.10"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "npx --no -- commitlint --edit ${1}"
    }
  },
  "lint-staged": {
    "**/src/**/*.{tsx,jsx,ts,js}": [
      "yarn lint:write"
    ]
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
